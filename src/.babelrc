{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  // babel插件一般都是以babel-plugin-xx开头,xx是功能化语义
  "plugins": [
    [
      "import", // 需安装babel-plugin-import插件：是一个用于按需加载组件代码和样式的 babel 插件
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css" // `style: true` 会加载 less 文件
      }
    ]
  ]
}
